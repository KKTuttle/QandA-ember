<h1> Question: {{model.content}} </h1>
<h2> Author: {{model.user}} </h2>
<h3> Notes: {{model.notes}} </h3>

  {{new-answer saveAnswer="saveAnswer" question=model}}


<h2> Answers for {{model.content}}</h2>

<ul>
  {{#each model.answers as |answer|}}
    {{answer-tile answer=answer}}
  {{/each}}
</ul>
